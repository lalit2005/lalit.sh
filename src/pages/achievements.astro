---
import { getEntry } from "astro:content";
import Layout from "../layouts/Layout.astro";

let entry = await getEntry("pages", "achievements");
let { Content } = await entry.render();
---

<Layout
  title="Achievements | Lalit"
  description="List of some of my achievements"
  image="/static/achievements-og.png"
>
  <div>
    <h1 class="title">Achievements</h1>
    <div class="md-content text-balance">
      <Content />
    </div>
  </div>
</Layout>
